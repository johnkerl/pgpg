{
  "version": "v0",
  "grammar": {
    "start_symbol": "expr",
    "rules": [
      { "lhs": "expr", "rhs": [ "term" ] },
      { "lhs": "expr", "rhs": [ "expr", "+", "term" ] },
      { "lhs": "term", "rhs": [ "factor" ] },
      { "lhs": "term", "rhs": [ "term", "*", "factor" ] },
      { "lhs": "factor", "rhs": [ "NUMBER" ] },
      { "lhs": "factor", "rhs": [ "(", "expr", ")" ] }
    ]
  },
  "lexer": {
    "rules": []
  },
  "parser": {
    "algorithm": "",
    "productions": [
      { "lhs": "expr", "rhs_count": 1 },
      { "lhs": "expr", "rhs_count": 3 },
      { "lhs": "term", "rhs_count": 1 },
      { "lhs": "term", "rhs_count": 3 },
      { "lhs": "factor", "rhs_count": 1 },
      { "lhs": "factor", "rhs_count": 3 }
    ]
  },
  "meta": {
    "source": "examples/arith.bnf"
  }
}
